{
    "name": "fast_condDiscAPActuallyStr_noBG_percept2LessWithAuto_preAuto2_aug",
    "cuda": true,
    "gpu": 0,
    "save_mode": "state_dict",
    "override": true,
    "super_computer":false,
    "data_loader": {
        "data_set_name": "AuthorHWDataset",

        "Xdata_dir": "/trainman-mount/trainman-storage-8308c0a4-7f25-47ad-ae22-1de9e3faf4ad",
        "data_dir": "../data/IAM/",
        "batch_size": 1,
        "a_batch_size": 4,
        "shuffle": true,
        "num_workers": 2,

        "img_height": 64,
        "max_width": 1300,
        "char_file": "./data/IAM_char_set.json",
        "mask_post": ["thresh","dilateCircle","errodeCircle"],
        "mask_random": false,
        "fg_masks_dir": "../data/IAM/fg_masks"
    },
    "validation": {
        "shuffle": false,
        "batch_size": 5,
        "a_batch_size": 4
    },

    
    "lr_scheduler_type": "none",
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.00001,
        "weight_decay": 0,
        "betas": [0.5,0.99]
    },
    "optimizer_type_discriminator": "Adam",
    "optimizer_discriminator": {
        "lr": 0.00001,
        "weight_decay": 0,
        "betas": [0,0.9]
    },
    "loss": {
        "auto": "pyramidL1Loss",
        "key": "pushMinDist",
        "count": "MSELoss",
        "mask": "HingeLoss",
        "perceptual": "L1Loss",
        "genRecog": "CTCLoss",
        "genAutoStyle": "L1Loss"
    },
    "loss_weights": {
        "auto": 0.1,
        "perceptual": 0.001,
        "discriminator": 1,
        "generator": 0.1,
        "key": 0.001,
        "count": 0.1,
        "mask": 0.1,
        "mask_generator": 0.01,
        "mask_discriminator": 0.01,
        "genRecog": 0.0001,
        "style_discriminator": 0.1,
        "style_generator": 0.01,
        "genAutoStyle": 0.01

    },
    "loss_params":
        {
            "auto": {"weights":[0.5,0.3,0.2],
                                "pool": "avg"},
            "key": {"dist":"l1",
                    "thresh": 1.0},
            "mask": {"threshold": 4}
        },
    "metrics": [],
    "trainer": {
        "class": "HWWithStyleTrainer",
        "iterations": 700000,
        "save_dir": "saved/",
        "val_step": 5000,
        "save_step": 25000,
        "save_step_minor": 250,
        "log_step": 100,
        "verbosity": 1,
        "monitor": "loss",
        "monitor_mode": "none",
        "space_input": true,
        "style_together": true,
        "use_hwr_pred_for_style": true,
        "hwr_without_style":true,
        "slow_param_names": ["keys"],
        "curriculum": {
            "0": [["auto"],["sample-disc"]],
            "1000": [["auto", "auto-gen"],["sample-disc"]],
            "10000": [["count","mask","gt_spaced","mask-gen"],["mask-disc"]],
            "19000": [["auto"],["count","mask","mask-gen"]],
            "20000": [  ["count","mask","mask-gen"],
                        ["auto","auto-gen"],
                        ["count","mask","mask-gen"],
                        ["auto","auto-mask","auto-gen"],
                        ["count","mask","mask-gen"],
                        [2,"sample-disc","mask-disc"],
                        [2,"gen","gen-auto-style"],
                        [2,"sample-disc","mask-disc"]]
        },
        "balance_loss": "sign_preserve",
        "interpolate_gen_styles": "extra-0.25",

	"text_data": "../data/english_text.txt",

        "use_learning_schedule": false,
        "no_bg_loss": true,
        "encoder_weights": "saved/IAM_auto_2/checkpoint-latest.pth",
        "encoder_type": "2"
    },
    "arch": "HWWithStyle", 
    "model": {
        "pretrained_autoencoder": "saved/IAM_auto_2/checkpoint-latest.pth",
        "num_class": 80,
        "generator": "PretrainedGen 2",
        "gen_dim": 32,
        "gen_n_res1": 1,
        "gen_n_res2": 1,
        "gen_n_res3": 1,
        "gen_use_skips": true,
	"hwr": "CRNN_group_norm_softmax",
        "pretrained_hwr": "saved/IAM_hwr_aug_softmax/checkpoint-latest.pth",
        "hwr_frozen": true,
        "style": "new",
        "use_pretrained_encoder": "2",
        "style_norm":"group",
        "style_activ":"relu",
        "style_dim": 128,
        "num_keys": 64,
        "global_pool": true,
        "discriminator": "condAP wide",
        "spacer": "duplicates",
        "create_mask": true,
        "mask_discriminator": "with derivitive"

    }
}
